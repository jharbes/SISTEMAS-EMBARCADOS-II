#include <main.h>
#include <lcd.c>

void main()
{

  lcd_init();
	lcd_putc("\fPartida...\n");

   while(TRUE)
   {

   if( input(pin_a0)){
   output_high(PIN_B0);
   lcd_putc("\fChave 1 = ligada\n");
   }
   else {
   output_low(PIN_B0);
   lcd_putc("\fChave 1 = desligada\n");
   }
   
   if( input(pin_a1)){
   output_high(PIN_B1);
   lcd_putc("\fChave 2 = ligada\n");
   }   
   else{
   output_low(PIN_B1);
   lcd_putc("\fChave 2 = desligada\n");
   }
       

   }

}
